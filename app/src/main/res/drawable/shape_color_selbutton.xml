<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="3dp"/>
            <solid android:color="#00000000" />
            <stroke
                android:color="#448AFF"
                android:width="2dp"/>

        </shape>
    </item>



    <!-- 最里层 -->
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape>
            <corners android:radius="3dp"/>
            <solid android:color="#ffffff" />
            <size
                android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>


</layer-list>